{
  "version": 2,
  "builds": [
    {
      "src": "realtime_tedx/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "realtime_tedx/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "realtime_tedx.settings",
    "DJANGO_DEBUG": "False"
  },
  "buildCommand": "bash build.sh"
}
